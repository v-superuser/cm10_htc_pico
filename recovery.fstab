# mount point   fstype    device        [device2]
 
/boot       mtd         boot            /dev/block/mtdblock2
/cache      yaffs2      cache
/data       yaffs2      userdata
/misc       mtd         misc            /dev/block/mtdblock0
/recovery   mtd         recovery        /dev/block/mtdblock1
/sdcard     auto        /dev/block/mmcblk0p1
/system     yaffs2      system
/sd-ext     ext4        /dev/block/mmcblk0p2
/devlog     mtd         devlog          /dev/block/mtdblock6
